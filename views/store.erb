<h2>Brands available at <%= @store.name() %></h2>


<% if @store_brands.any?() %>
  <% @store_brands.each()do |brand| %>
    <div class="thumbnail" href="#"><img src="/img/<%= brand.image() %>" alt="Adidas" class="img-responsive"/></div>
  <% end  %>
<% end  %>

<div class="bottom">
  <a class="btn btn-default" href="/">Back</a>
</div>
